/* pages/fenlei/fenlei.wxss */

.Container {
  background-color: #f5f5f5;
  height: 100%;
}

.nav {
  padding: 25rpx 40rpx;
  background-color: #ffa3cb;
  font-size: 1rem;
  color: #fff;
}

/* 头部返回结束 */

.list {
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  padding-top: 20rpx;
}

.list_a {
  flex-basis: 30%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  padding: 20rpx 0;
  font-size: 14px;
  color: #666;
}

.image_frame {
  width: 150rpx;
  height: 150rpx;
  border: 0;
  border-radius: 50%;
  margin-bottom: 20rpx;
}

/* 列表结束 */
.btn {
  border: 0;
  width: 50%;
  margin-top: 50rpx;
  margin-bottom: 50%;
  background-color: #ffa3cb;
  color: #fff;
  font-size: 14px;
}

/* 搜索按钮结束 */
