.Container {
  background-color: #efefef;
}

.zwsp {
  padding: 300rpx 0;
  text-align: center;
  color: #888;
}

.nav {
  padding: 25rpx 40rpx;
  background-color: #ffa3cb;
  font-size: 17px;
  color: #fff;
}

/* 头部返回结束 */

.navber {
  display: flex;
  flex-wrap: wrap;
  background-color: #fff;
  padding: 20rpx 50rpx;
}

.navber_list {
  display: flex;
  justify-content: center;
  flex: 4;
  font-size: 14px;
}

/* 导航结束 */

.list {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  padding: 20rpx;
  width: 80%；;
}

.list_a {
  display: flex;
  flex-basis: 43%;
  flex-direction: column;
  padding: 20rpx;
  margin-bottom: 20rpx;
  border: 0;
  border-radius: 10rpx;
  background-color: #fff;
}

.image_frame {
  width: 100%;
}

.text_a {
  overflow: hidden;
  width: 100%;
  height: 70rpx;
  font-size: 12px;
}

.text_b {
  width: 100%;
  height: 60rpx;
  overflow: hidden;
  font-size: 10px;
}

.list_b {
  display: flex;
  justify-content: space-between;
  padding: 10rpx 0;
}

.icon-wujiaoxingkong {
  font-size: 10px;
  color: red;
}

