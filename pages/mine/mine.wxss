/* pages/mine/mine.wxss */

.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
  position: relative;
}

.resbtn {
  padding: 0;
  margin: 0;
  border: unset;
  border-radius: 0;
  background-color: #fff;
  line-height: unset;
  outline: unset;
  box-shadow: unset;
}
.resbtn::after{
  border: none;
}
.account-bg {
  width: 100%;
  height: 300rpx;
}

.userinfo_a {
  position: absolute;
  display: flex;
  justify-content: space-between;
  top: 70rpx;
  left: 100rpx;
}

.userinfo_b {
  display: flex;
  flex-direction: column;
  padding: 50rpx;
}

.text_a {
  font-size: 17px;
  color: #767676;
}

.text_b {
  font-size: 14px;
  color: #767676;
}

.image_frame {
  width: 180rpx;
  height: 180rpx;
  border: 0;
  border-radius: 50%;
}

.userinfo-avatar {
  width: 108rpx;
  height: 108rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #fff;
}

/* 订单 */

.order {
  display: flex;
  flex-direction: row;
  align-items: center;
  width: 100%;
  height: 90rpx;
}

.myorder-text {
  font-size: 30rpx;
  color: gray;
  margin: 20rpx;
  width: 40%;
}

.myorderlook-text {
  font-size: 30rpx;
  color: gray;
  position: relative;
  right: 20rpx;
  width: 60%;
  text-align: right;
}

.next-image {
  width: 20rpx;
  height: 25rpx;
  position: relative;
  right: 10rpx;
}

.navs {
  display: flex;
  justify-content: space-between;
  padding: 20rpx 0;
}

.navs_left {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 65%;
  padding: 0 20rpx;
  background-color: #fff;
  font-size: 14px;
}

.nav-item {
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
  padding: 4rpx;
}

.navs_right {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin-left: 10rpx;
  width: 25%;
  padding: 40rpx;
  background-color: #fff;
  font-size: 14px;
}

/* 列表 */

.center {
  background-color: #fff;
}

.list-item {
  display: flex;
  align-items: center;
  height: 80rpx;
  margin: 0 40rpx;
  border-bottom: 1px solid#c4c4c4;
}

.list {
  display: flex;
  justify-content: space-between;
  width: 80%;
  /* padding: 20rpx 0; */
}

.item-text {
  color: #505050;
  font-size: 14px;
  text-align: center;
}

.icon-arrow-right {
  color: #888;
}

.item-text_a {
  color: #505050;
  font-size: 12px;
}

/* .icon-arrow-right {
   padding: 20rpx 50rpx 0 530rpx; 
} */

.list_left {
  width: 15%;
  text-align: center;
  color: #d81e06;
}

/* 列表结束 */

