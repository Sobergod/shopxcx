<!--pages/youhuiquan/youhuiquan.wxml-->
<view wx:if='{{!status}}' class='no-con'>
  <image src='../../images/noyhq.png'></image>
  <text>暂无优惠券,快去领取吧!</text>
</view>
<view wx:else class='bg'>
  <block wx:for="{{menu_b}}">
    <view class='list_c'>
      <view class='tw'>
        <image class="tpa" src="{{item.tpUrl}}" mode="aspectFill">
        </image>
        <view class='bj'>
          <view class='text_b'>满{{item.limitmoney}}</view>
          <view class='text_c'>减{{item.money}} </view>
          <view style='font-size:14px;'> 使用时间:{{item.limitdat}}前使用</view>
        </view>
      </view>
    </view>
  </block>
</view>