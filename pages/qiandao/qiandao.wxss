.navber {
  display: flex;
  justify-content: space-between;
  padding: 25rpx 40rpx;
  background-color: #fff;
  font-size: 14px;
  margin-bottom: 20rpx;
}
.daka {
  font-size: 14px;
  line-height: 30px;

}
.denglu {
  font-size: 12px;
  padding-right: 10rpx;
