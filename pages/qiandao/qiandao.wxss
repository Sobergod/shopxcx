.list_a {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  background: #fff;
}

.text_a {
  padding-top: 50rpx;
  font-size: 17px;
}

.text_b {
  padding-top: 50rpx;
  font-size: 20px;
