/* pages/zuji/zuji.wxss */
.no-collection {
  margin-top: 150px;
  text-align: center;
  font-size: 14px;
  color: #888;
}
.no-collection image {
  width: 50px;
  height: 50px;
}
.center {
  display: flex;
  flex-wrap: wrap;
}
.list {
  width: 46%;
  background-color: #fff;
  margin: 2%;

}
.text_name {
  display: block;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  padding: 10rpx;
  font-size: 14px;
}

.image_frame {
  width: 100%;
  height: 350rpx;
}
.fenlan {
  display: flex;
  justify-content: space-between;
  padding: 20rpx 10rpx;
}
.text_money{
  font-size: 14px;
  color: #ff0000;
}
.text_shoucang {
  font-size: 12px;
  color: #313131;
}
.icon-wujiaoxingkong {
  color: #ff0000;
}
/* 列表结束 */
.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-wujiaoxingkong:before { content: "\e646"; }

